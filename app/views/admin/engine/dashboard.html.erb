<div class='panel panel-default'>
  <div class='panel-heading'>
    <h3 class='panel-title'><%= t('.gathering_nutrition_facts') %></h3>
  </div>

  <div class='panel-body'>
    <%= form_tag(fetch_admin_ingredients_path) do %>
      <div class='form-inline'>
        <div class='form-group'>
          <%= text_field_tag 'name', params[:url], class: 'form-control', placeholder: 'Ingredient name' %>
        </div>
        <div class='form-group'>
          <%= url_field_tag 'url', params[:url], class: 'form-control', placeholder: 'Url', type: 'url' %>
        </div>
        <%= submit_tag 'Start', class: 'btn btn-default' %>
      </div>
    <% end %>
  </div>
</div>
